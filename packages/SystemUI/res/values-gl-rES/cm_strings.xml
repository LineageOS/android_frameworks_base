  <!-- [CHAR LIMIT=NONE] Importance Tuner setting title -->
  <string name="cm_power_notification_controls_description">Cos controis das notificacións de enerxía, podes definir un nivel de importancia entre 0 e 6 para as notificacións dunha aplicación.
        \n\n<b>Nivel 6</b>
        \n- Amosar na parte superior da lista de notificacións
        \n- Permitir interrupcións a pantalla completa
        \n- Sempre intrusivas
        \n\n<b>Nivel 5</b>
        \n- Previr interrupcións a pantalla completa
        \n- Sempre intrusivas
        \n\n<b>Nivel 4</b>
        \n- Previr interrupcións a pantalla completa
        \n- Nunca intrusivas
        \n\n<b>Nivel 3</b>
        \n- Previr interrupcións a pantalla completa
        \n- Nunca intrusivas
        \n- Nunca reproducir sons ou vibrar
        \n\n<b>Nivel 2</b>
        \n- Previr interrupcións a pantalla completa
        \n- Nunca intrusivas
        \n- Nunca reproducir sons ou vibrar
        \n- Ocultar na pantalla de bloqueo
        \n- Amosar ao final da lista de notificacións
        \n\n<b>Nivel 1</b>
        \n- Previr interrupcións a pantalla completa
        \n- Nunca intrusivas
        \n- Nunca reproducir sons ou vibrar
        \n- Ocultar na pantalla de bloqueo e na barra de estado
        \n- Amosar ao final da lista de notificacións
        \n\n<b>Nivel 0</b>
        \n- Bloquear todas as notificacións
    </string>
